{
    "name": "devplus31/furgonetka-laravel",
    "description": "Furgonetka Laravel",
    "type": "library",
    "require": {
        "php": "^8.1",
        "illuminate/support": ">=11.0",
        "kwarcek/furgonetka-rest-api-php": "dev-master"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "DevPlus31\\Furgonetka\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Mustapha Ben",
            "email": "devplus31@gmail.com"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "DevPlus31\\Furgonetka\\Providers\\FurgonetkaServiceProvider"
            ]
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/DevPlus31/furgonetka-rest-api-php"
        }
    ],
    "prefer-stable": true,
    "config": {
        "sort-packages": true
